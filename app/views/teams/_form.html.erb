<%= simple_form_for @team do |f| %>
  <%= f.input :name %>
  <%= f.association :hospital, collection: @user.hospitals %>
  <%= f.association :members, label: "Add members", input_html: { mulitple: true } %>
  <%= f.input :user_id, as: "hidden", input_html: { value: @user.id } %>
  <%= f.submit "Submit", class: "button small radius" %>
<% end %>
